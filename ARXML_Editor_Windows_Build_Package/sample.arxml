<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_4-7-0.xsd">
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>RootPackage</SHORT-NAME>
      <ELEMENTS>
        <APPLICATION-SW-COMPONENT-TYPE>
          <SHORT-NAME>TestAppComponent</SHORT-NAME>
          <DESC>
            <L-2 L="EN">A test application component</L-2>
          </DESC>
          <PORTS>
            <P-PORT-PROTOTYPE>
              <SHORT-NAME>TestPort</SHORT-NAME>
              <DESC>
                <L-2 L="EN">A test provider port</L-2>
              </DESC>
              <PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/RootPackage/TestInterface</PROVIDED-INTERFACE-TREF>
            </P-PORT-PROTOTYPE>
          </PORTS>
        </APPLICATION-SW-COMPONENT-TYPE>
        
        <SENDER-RECEIVER-INTERFACE>
          <SHORT-NAME>TestInterface</SHORT-NAME>
          <DESC>
            <L-2 L="EN">A test sender-receiver interface</L-2>
          </DESC>
          <DATA-ELEMENTS>
            <DATA-ELEMENT-PROTOTYPE>
              <SHORT-NAME>TestData</SHORT-NAME>
              <DESC>
                <L-2 L="EN">Test data element</L-2>
              </DESC>
              <TYPE-TREF DEST="APPLICATION-PRIMITIVE-DATA-TYPE">/RootPackage/TestDataType</TYPE-TREF>
            </DATA-ELEMENT-PROTOTYPE>
          </DATA-ELEMENTS>
        </SENDER-RECEIVER-INTERFACE>
      </ELEMENTS>
    </AR-PACKAGE>
  </AR-PACKAGES>
</AUTOSAR>